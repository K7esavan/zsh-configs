export ZDOTDIR=$HOME/.config/zsh_configs

# Variables
# source $ZDOTDIR/variables.zsh

source $ZDOTDIR/chriszshrc.zsh
# Prompt
# source $ZDOTDIR/prompt.zsh

# History
source $ZDOTDIR/history.zsh

# Tab completion
source $ZDOTDIR/tabcompletion.zsh

# Vim mode
source $ZDOTDIR/vimode.zsh

# Aliases
source $ZDOTDIR/aliases.zsh

# My collection of binaries
source $ZDOTDIR/bin

# Load zsh-syntax-highlighting; should be last.
source $ZDOTDIR/syntaxhighlighting.zsh


